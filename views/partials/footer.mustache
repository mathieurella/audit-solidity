<footer>
    <div class="row py-md-4">
        <div class="col-md-5 col-sm-12 text-left">
            <small class="copyright mb-0">{{global.copyright}}</small>
        </div>
        <div class="col-md-4 col-sm-12 text-center">
            <ul class="list-inline social social-white-alt mb-0">
                {{#global.social.facebook.show_or_hide}}
                <li class="list-inline-item"><a href="{{{global.social.facebook.link}}}" target="_blank"><i class="fab fa-facebook-f"></i></a></li>
                {{/global.social.facebook.show_or_hide}}

                {{#global.social.instagram.show_or_hide}}
                <li class="list-inline-item"><a href="{{{global.social.instagram.link}}}" target="_blank"><i class="fab fa-instagram"></i></a></li>
                {{/global.social.instagram.show_or_hide}}

                {{#global.social.twitter.show_or_hide}}
                <li class="list-inline-item"><a href="{{{global.social.twitter.link}}}" target="_blank"><i class="fab fa-twitter"></i></a></li>
                {{/global.social.twitter.show_or_hide}}

                {{#global.social.youtube.show_or_hide}}
                <li class="list-inline-item"><a href="{{{global.social.youtube.link}}}" target="_blank"><i class="fab fa-youtube"></i></a></li>
                {{/global.social.youtube.show_or_hide}}
            </ul>
        </div>
        <div class="col-md-3 col-sm-12 text-right">
            <p class="vertex mb-0"><a href="https://www.turbo360.co/" target="_blank"><img class="img-logo-footer" src="https://lh3.googleusercontent.com/Nd9cMfIKz4VGQ2inBjBkdhsnOexIHpYMDrefPwFpGM5IU_-LryDqpyC4LaTAuA6Jm0z__jHxe6SheitNHTZIkzd2gA"/></a></p>
        </div>
    </div>
</footer>
